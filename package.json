{
  "name": "todomvc",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:dockerenv": "cross-env npm run build && mv dist server/public",
    "build:local": "cross-env npm run build && rimraf server/public/dist && mv dist server/public",
    "docker:hub": "bash ./docker-build-push.sh",
    "azure": "bash ./azure-deploy.sh",
